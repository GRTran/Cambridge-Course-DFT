import numpy as np
import matplotlib.pyplot as plt

x = []
x2 = []
y1 = []
y2 = []

for i in range(101,800):
    j = i / 100.0
    x += [j]
    y1 += [1.0/(j-1.0)]

for i in range(0,8000):
    j = i/1000.0
    x2 += [j]
    y2 += [(8.0 + 4.0*j + 2.0*j**2 + j**3) / (8.0 + 4.0*j + 2.0*j**2 + j**3 + j**4)]

print(x2)

plt.plot(x,y1)
plt.plot(x2,y2)
plt.ylim(0,2.5)
plt.xlim(0,8.0)
plt.xlabel('Normalised Hamiltonian diagonal value')
plt.ylabel('Precondition Cell Value')
plt.show()
